{
  "name": "lunatrack-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "http": "node --experimental-specifier-resolution=node ./dist/start.js http",
    "https": "node --experimental-specifier-resolution=node ./dist/start.js https",
    "dev:http": "tsx src/start.ts http",
    "dev:https": "tsx src/start.ts https"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
